


#include<stdio.h>
void main(){
	int a,b,i,j;
	printf("Rows: ") && scanf("%d",&a);
	printf("Columns: ") && scanf("%d",&b);
	if(a==0||b==0){
		printf("Invalid dimensions\n");
	}else{
		printf("Seating Arrangement Matrix:\n");
		for(i=1;i<=a;i++){
			for(j=1;j<=b;j++){
				printf("R%dC%d ",i,j);
			}
			printf("\n");
		}
	}
}

/*


Sample Test Cases
Test case 1
Rows:·3	
Columns:·4	
Seating·Arrangement·Matrix:⏎	
R1C1·R1C2·R1C3·R1C4·⏎	
R2C1·R2C2·R2C3·R2C4·⏎	
R3C1·R3C2·R3C3·R3C4·⏎	
Test case 2
Rows:·0	
Columns:·3	
Invalid·dimensions⏎

*/
