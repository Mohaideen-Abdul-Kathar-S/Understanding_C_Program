


#include<stdio.h>
#include<string.h>
void main(){
	char a[100];
	int n,i;
	scanf("%s",&a);

	for(i=0;i<=strlen(a);i++){
		if(islower(a[i])){
			a[i]=toupper(a[i]);
		}else {
			a[i]=tolower(a[i]);
		}
	}
	printf("%s",a);
}

