

#include<stdio.h>
#include<string.h>
void main(){
	char a[100];
	int i,j;
	gets(a);
	for(i=0;i<strlen(a);i++){
		for(j=0;j<=i;j++){
			printf("%c",a[j]);
		}
		printf("\n");
	}
}


/*


