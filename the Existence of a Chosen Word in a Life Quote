
//Determining the Existence of a Chosen Word in a Life Quote

#include<stdio.h>
#include<string.h>
void main(){
	char a[100],b[100];
	gets(a);
	gets(b);
	if(strstr(a,b)) puts("Exists");
	else puts("Dosen't Exists");
}

