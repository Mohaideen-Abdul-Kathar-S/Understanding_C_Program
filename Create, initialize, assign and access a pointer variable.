


#include <stdio.h>
void main() {
	int num1, num2;
	int *a, *b;
printf("Enter two int values : ") && scanf("%d %d",&num1,&num2);
a=&num1;
b=&num2;

	printf("Sum of two integers %d and %d = %d\n", *a, *b, *a + *b);
}

/*


Sample Test Cases
