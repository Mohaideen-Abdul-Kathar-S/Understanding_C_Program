
//Calculating Differences or Sums based on Number Pairs

#include<stdio.h>
void main(){ int a,b;
	scanf("%d %d",&a,&b);
	if(a>b) printf("%d\n",a-b);
	else printf("%d\n",a+b);
}


/*


