//Total Fishes


#include<stdio.h>
void main(){ int a;
		printf("Enter the time in minutes : ") && scanf("%d",&a);
		if(a<=0) printf("Invalid Time\n");
		else
		printf("Total fishes : %d\n",a*2);
		   }


