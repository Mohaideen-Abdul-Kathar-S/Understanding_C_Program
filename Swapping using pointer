


#include<stdio.h>
void swap(int *p1,int *p2){
	int a=*p2;
	*p2=*p1;
	*p1=a;
}


void main(){
	int num1,num2;
	scanf("%d %d",&num1,&num2);
	printf("Before swapping: %d %d\n",num1,num2);
	swap(&num1,&num2);
	printf("After swapping: %d %d\n",num1,num2);
}


/*


Sample Test Cases
Test case 1
30 90	
Before·swapping:·30·90⏎	
After·swapping:·90·30⏎	
Test case 2
-12345 98765	
Before·swapping:·-12345·98765⏎	
After·swapping:·98765·-12345⏎

*/
